{
    "name": "@vibe-test/lib",
    "version": "1.0.0",
    "description": "Auto-generating Postman-like UI for Express applications",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "ui",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build:lib": "tsc",
        "build:ui": "cd ui-src && npm install && npm run build",
        "build": "npm run build:lib && npm run build:ui",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "lint:ui": "cd ui-src && npm run lint",
        "lint:all": "npm run lint && npm run lint:ui",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "express",
        "api",
        "testing",
        "postman",
        "swagger",
        "rest",
        "api-testing",
        "api-documentation",
        "express-middleware",
        "api-tester",
        "http-client",
        "request-testing",
        "api-explorer",
        "developer-tools"
    ],
    "author": {
        "name": "Prathamesh Chavan",
        "email": "cprathmesh01@gmail.com",
        "url": "https://github.com/01Pratham"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/01Pratham/vibe-test.git"
    },
    "bugs": {
        "url": "https://github.com/01Pratham/vibe-test/issues"
    },
    "homepage": "https://github.com/01Pratham/vibe-test#readme",
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/01Pratham"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "http-status": "^1.7.3"
    },
    "peerDependencies": {
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.16",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.56.0",
        "eslint-config-next": "^14.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "express": "^4.18.2",
        "typescript": "^5.3.3"
    }
}